{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "Realm": "emma",
    "NameClaimType": "preferred_username",
    "ValidAudience": "account",
    "ValidateIssuer": true
  },
  "Integrations": {
    "Shelly": {
      "IntegratorTag": "ITG_OCE"
    }
  }
}
