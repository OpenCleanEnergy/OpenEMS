{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "DotNetCore.CAP": "Information",
        "OpenEMS.Integrations.Shelly.Infrastructure.ShellyWebsocketMessageHandler": "Debug",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    }
  },
  "Sentry": {
    "Debug": false,
    "TracesSampleRate": 0.2
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "Realm": "oce",
    "NameClaimType": "preferred_username",
    "ValidAudience": "account"
  },
  "Analytics": {
    "Sampling": {
      "SamplingInterval": "00:03:00"
    }
  }
}
