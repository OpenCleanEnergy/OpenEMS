{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Websocket.Client": "Information"
      }
    }
  },
  "OpenTelemetry": {
    "Endpoint": "http://localhost:4317",
    "Protocol": "gRPC"
  },
  "Sentry": {
    "Debug": false,
    "TracesSampleRate": 1.0,
    "MinimumBreadcrumbLevel": "Debug"
  },
  "Keycloak": {
    "AuthServerUrl": "http://localhost:5001",
    "SwaggerClientId": "swagger"
  },
  "Database": {
    "Host": "localhost",
    "Database": "dev",
    "Username": "dev",
    "Password": "dev",
    "SslRequired": false
  },
  "Events": {
    "RabbitMQ": {
      "Url": "amqp://dev:dev@localhost:5672"
    }
  },
  "Analytics": {
    "Sampling": {
      "SamplingInterval": "00:01:30"
    }
  },
  "Integrations": {
    "Shelly": {
      "IsEnabled": false
    }
  }
}
